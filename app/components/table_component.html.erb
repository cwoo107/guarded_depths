<div class="flex flex-col">
  <div class="my-6 overflow-x-auto sm:-mx-2 lg:-mx-4">
    <div class="py-2 align-middle inline-block min-w-full sm:px-6 lg:px-8">
      <div class=" overflow-hidden border-b border-gray-200 sm:rounded-lg">
        <table class="min-w-full divide-y divide-gray-200">
          <thead class="bg-gray-50">
          <tr>
            <% if @data.klass == Ticket %>
              <th scope="col"
                  class="px-2 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
              >
                Member Code
              </th>
              <th scope="col"
                  class="px-2 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
              >
                Ticket
              </th>
              <th scope="col"
                  class="px-2 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
              >
                Type
              </th>
              <th scope="col"
                  class="px-2 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
              >
                Priority
              </th>
            <% end %>
            <% @data.first.attributes.keys.each do |key| %>
              <th scope="col"
                  class="px-2 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
              >
                <%= key unless key == 'user_id' || key == 'id'  || key == 'updated_at' || key == 'ticket' || key == 'code_id'%>
              </th>
            <% end %>
            <% if @links %>
              <th scope="col"
                  class="px-2 py-2 text-left text-xs font-medium text-gray-500 uppercase tracking-wider"
              >
                View Details
              </th>
            <% end %>
          </tr>
          </thead>
          <tbody class=" ">
          <% @data.each do |data| %>
            <tr class="<%= cycle('bg-white', 'bg-gray-50') %> hover:bg-gray-200">
              <% if @data.klass == Ticket %>
                <td class="p-2 m-4 whitespace-nowrap text-sm ">
                  <%= data.attributes['ticket']['NewDataSet']['delivery']['recipient'] %>
                </td>
                <td class="p-2 m-4 whitespace-nowrap text-sm ">
                  <%= "#{data.attributes['ticket']['NewDataSet']['tickets']['ticket']}-#{data.attributes['ticket']['NewDataSet']['tickets']['revision']}" %>
                </td>
                <td class="p-2 m-4 whitespace-nowrap text-sm ">
                  <%= data.attributes['ticket']['NewDataSet']['tickets']['type'] %>
                </td>
                <td class="p-2 m-4 whitespace-nowrap text-sm ">
                  <%= data.attributes['ticket']['NewDataSet']['tickets']['priority'] %>
                </td>
              <% end %>
              <% data.attributes.each do |key, value| %>
                <td class="p-2 m-4 whitespace-nowrap text-sm ">
                  <%= value unless key == 'user_id' || key == 'id' || key == 'updated_at' || key == 'ticket' || key == 'code_id' %>
                </td>
              <% end %>
              <% if @links %>
                <td class="">

                  <%= link_to 'View', data %>
                </td>
              <% end %>
            </tr>

          <% end %>

          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
